{
  "name": "adash-rn-web",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "concurrently --kill-others \"expo start --web\" \"npx ts-node server\"",
    "dev:electron": "electron-webpack dev",
    "server": "npx ts-node server.ts",
    "start": "expo start",
    "build": "expo build:web --no-pwa",
    "build:electron": "NODE_ENV=production yarn electron-webpack && yarn electron-builder --dir -c.compression=store -c.mac.identity=null",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts*\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@fastify/compress": "^5.0.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "adash-ts-helper": "^1.0.5",
    "axios": "^1.6.4",
    "chart.js": "^3.7.1",
    "date-fns": "^2.28.0",
    "dom-to-image": "^2.6.0",
    "electron-store": "^8.0.1",
    "expo": "~44.0.0",
    "expo-asset": "~8.4.6",
    "expo-status-bar": "~1.2.0",
    "fastify": "^3.29.0",
    "fastify-cors": "^6.1.0",
    "fastify-static": "^4.7.0",
    "html2canvas": "^1.4.1",
    "js-file-download": "^0.4.12",
    "jsonpack": "^1.1.5",
    "lodash": "^4.17.21",
    "modal-react-native-web": "^0.2.0",
    "native-base": "^3.4.3",
    "react": "17.0.1",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.31.1",
    "react-native": "0.64.3",
    "react-native-draggable-gridview": "^1.0.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-table-component": "^1.2.2",
    "react-native-web": "0.17.1",
    "simple-git": "^3.7.1",
    "store2": "^2.13.2",
    "swr": "^1.3.0",
    "usehooks-ts": "^2.5.1",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@expo/electron-adapter": "^0.0.55",
    "@expo/webpack-config": "^0.16.23",
    "@types/lodash": "^4.14.182",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-table-component": "^1.2.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "electron": "^6.0.12",
    "electron-builder": "^23.0.3",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react-native-themed-styles": "^0.0.4",
    "typescript": "^4.6.4"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "private": true,
  "build": {
    "extraMetadata": {
      "main": "main.js"
    },
    "files": [
      {
        "from": "dist/main/",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "dist/renderer",
        "to": "./",
        "filter": [
          "**/*"
        ]
      },
      "package.json",
      "**/node_modules/**/*"
    ]
  }
}
