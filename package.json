{
  "name": "adash-rn-web",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "notificator": "cd ../adash-data-collector && DEBUG=1 node ./bin/adash-data-collector notificator --config=./config.json && cp ./data/*.json ../adash-rn-web/src/data/",
    "collector": "cd ../adash-data-collector && DEBUG=1 node ./bin/adash-data-collector collect && cp ./data/*.json ../adash-rn-web/src/data/",
    "computekpi": "cd ../adash-data-collector && DEBUG=1 node ./bin/adash-data-collector computekpi && cp ./data/kpie2e.json ../adash-rn-web/src/data/kpie2e.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts*\" --write",
    "fix:lint": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "axios": "^0.24.0",
    "chart.js": "^3.6.2",
    "date-fns": "^2.27.0",
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "global": "^4.4.0",
    "html2canvas": "^1.4.0",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "modal-react-native-web": "^0.2.0",
    "native-base": "^3.2.2",
    "react": "17.0.1",
    "react-chartjs-2": "^4.0.0",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.1.4",
    "react-native": "0.64.3",
    "react-native-draggable-gridview": "^1.0.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-table-component": "^1.2.2",
    "react-native-web": "0.17.1",
    "usehooks-ts": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/lodash": "^4.14.178",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-table-component": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react-native-themed-styles": "^0.0.4",
    "typescript": "~4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "private": true
}
